{
  "name": "xtatix",
  "description": "A powerful CSS Orchastrator for building scalable and high-performance CSS frameworks. Seamlessly blend the raw potential of CSS with the ease of pre-made utilities, managed by a core orchestrator.",
  "version": "0.0.27",
  "compilerVersion": "0.0.27",
  "main": "./execute/index.js",
  "types": "./execute/index.d.ts",
  "exports": "./execute/index.js",
  "typings": "./execute/index.d.ts",
  "bin": {
    "xtx": "./execute/index.js",
    "xtatix": "./execute/index.js",
    "xdv": "./compiler/scripts/live.sh"
  },
  "scripts": {
    "sync": "./compiler/scripts/sync.sh",
    "postinstall": "chmod +x ./execute/index.js && ./execute/index.js",
    "scaffold:link": "ln -s \"$(pwd)/scaffold\" ../xcaffold",
    "profile": "go install github.com/moderato-app/live-pprof@latest && live-pprof",
    "lint": "cd source && golangci-lint run && cd ..",
    "build": "cd compiler && chmod +x ./scripts/build.sh && ./scripts/build.sh && cd ..",
    "stats": "chmod +x ./compiler/scripts/stats.sh && ./compiler/scripts/stats.sh compiler",
    "bundle": "npm pack --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yshelldev/xtatix-central"
  },
  "keywords": [
    "css",
    "xtatix",
    "styles",
    "semantic",
    "frontend",
    "design",
    "framework"
  ],
  "author": {
    "name": "Vyshnav Prasad",
    "url": "https://www.yshell.dev"
  },
  "license": "Custom",
  "licenseFile": "LICENSE.md",
  "homepage": "https://www.xtatix.io",
  "bugs": {
    "url": "https://github.com/yshelldev/xtatix-central/issues"
  },
  "contributing": "https://github.com/yshelldev/xtatix-central/blob/main/CONTRIBUTING.md",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/yshelldev"
  },
  "type": "module"
}